<%- include('partials/header'); %>
<div>
<% if (applications.length > 0) { %>
	<table class="table">
		<thead class="thead-dark">
			<tr>
				<th>Program</th>
                <th>Student</th>
                <th>Action</th>
			</tr>
		</thead>
		<tbody>
		<% for (let i = 0; i < applications.length; i++) { %>
			<tr>
				<td><%= applications[i].program %></td>
                <td><%= applications[i].student%></td>
                <td>
					<a href="" class="btn btn-sm btn-success">Accept</a>
					<a href="" class="btn btn-sm btn-danger">Deny</a>
				</td>
			</tr>
		<% } %>
		</tbody>
	</table>
<% } else { %>
	<p class="text-center">No applications found.</p>
<% } %>
</div>
</div>
</body>
</html>